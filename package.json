{
  "name": "victim-services",
  "version": "1.0.14",
  "description": "NYC Crime Victim Services Finder",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "build-no-test": "yarn roll-css && webpack",
    "prebuild": "yarn test && yarn roll-css",
    "roll-css": "./node_modules/nyc-lib/css/roll-css.sh ./src/css/victim-services.theme.css ./src/css/victim-services.css",
    "predeploy": "yarn build",
    "deploy": "node deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CityOfNewYork/victim-services.git"
  },
  "author": "Tim Keane/Christopher Menedes",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/CityOfNewYork/victim-services/issues"
  },
  "homepage": "https://github.com/CityOfNewYork/victim-services#readme",
  "dependencies": {
    "jquery": "^3.5.0",
    "nyc-lib": "^1.4.39",
    "ol": "^6.5.0",
    "papaparse": "^5.2.0",
    "proj4": "^2.4.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "coveralls": "^3.0.2",
    "jest": "^24.8.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-fetch-mock": "^1.6.5",
    "nyc-build-helper": "^0.0.39",
    "webpack": "4.19.1",
    "webpack-cli": "^3.3.11"
  },
  "resolutions": {
    "**/**/serialize-javascript": "^3.1.0",
    "**/**/node-fetch": "^2.6.1",
    "**/**/ssri": "6.0.2",
    "**/**/postcss": "^8.2.10"
  },
  "jest": {
    "verbose": true,
    "automock": false,
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "setupFiles": [
      "./__tests__/jest-setup.js"
    ],
    "transformIgnorePatterns": [],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/js/**/*.js"
    ]
  }
}
